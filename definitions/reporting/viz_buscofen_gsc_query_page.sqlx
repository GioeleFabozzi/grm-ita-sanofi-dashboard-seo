config {
  type: "table"
}

WITH temp_viz_gsc_query_page AS (
SELECT 
    *,
    CASE WHEN LOWER(query) LIKE '%buscof%' THEN 'Brand' ELSE 'Generic' END AS query_type
FROM 
    ${ref('buscofen_gsc_query_page')}
)

SELECT 
    *
FROM 
    temp_viz_gsc_query_page 
ORDER BY 
    date, query, page, site_url