    ga_device_category,
    ga_page,
    ga_goal_type

 UNION ALL 

SELECT   
    ga_date,
    ga_channel_grouping,
    ga_source_medium,
    ga_device_category,
    ga_page,
    'ga_goal_click_cerca_medico' AS ga_goal_type,
