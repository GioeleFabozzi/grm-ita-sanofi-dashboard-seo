    SUM(CAST(volume AS FLOAT64)) as total_volume
FROM 
    ${ref('raw_enterogermina_semrush_overview')}
WHERE 
    yovis_landing is not null 
GROUP BY     
   keyword,
   -- landing,
   tag
    
UNION ALL

SELECT 
   'multicentrum' AS source,
