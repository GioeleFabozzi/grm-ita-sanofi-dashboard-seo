config {
  type: "table"
}

WITH temp_viz_gsc_query_page AS (
SELECT 
    *,
    CASE WHEN LOWER(query) LIKE '%entero%' THEN 'Brand' ELSE 'Generic' END AS query_type
FROM 
    ${ref('enterogermina_gsc_query_page')}
)

SELECT 
    *
FROM 
    temp_viz_gsc_query_page 
WHERE
    page LIKE '%/it-it%'
ORDER BY 
    date, query, page, site_url